<%= form_for @pay_table, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="form-group">
    <%= f.label :count, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :count, :class => 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :stamp_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :stamp_id, :class => 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :slot_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :slot_id, :class => 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :points, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :points, :class => 'form-control' %>
    </div>
  </div>

  
  <%= f.submit nil, :class => 'btn btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
            pay_tables_path, :class => 'btn btn-default' %>
 
<% end %>
